START=99

SERVICE_USE_PID=1

start() {
	#mkdir -m 0755 -p /var/log/tinyaprs
	#echo "Starting tinyaprs"
	/usr/sbin/tinyaprs -d
}

stop() {
	PID=`cat /tmp/tinyaprs.pid`
    kill $PID > /dev/null 2>&1
    #exit 0
}

restart() {
	stop
	start
}
